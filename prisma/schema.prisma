generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Repository {
  id          String   @id @default(cuid())
  name        String
  url         String
  description String?
  username    String?
  password    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
